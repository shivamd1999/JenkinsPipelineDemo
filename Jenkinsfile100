pipeline{
    agent any
    
    stages{
        stage("Env "){
            steps{
                echo "The build number is ${env.BUILD_NUMBER}"
                echo "You can also use \${BUILD_NUMBER} -> ${BUILD_NUMBER}"                                                
            }
        }
            steps{
                echo "The build number is ${env.JOB_NAME}"
                echo "You can also use \${JOB_NAME} -> ${JOB_NAME}"                                                
            }
        }     
    }
}
