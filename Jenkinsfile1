pipeline {
    agent { label 'Slave' }
    
    stages {
        stage('Python File') {
            steps {
                bat 'hello1.py'
            }
        }
        stage('batch') {
            steps {
                echo 'hello.bat'
            }
        }
        stage('Release') {
            steps {
                echo 'Releasing'
            }
        }
    }
}
