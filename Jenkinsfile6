pipeline {
    agent any
        stages {
        stage('Bulding') {
            steps {
                bat 'shivam.py'
            }
        }
    }
    post {
        always {
            
                echo 'Status of build'
                
                publishHTML([allowMissing: false, alwaysLinkToLastBuild: true, keepAll: false, reportDir: 'D:\\email.html', reportFiles: 'index.html', reportName: 'HTML Report', reportTitles: ''])

                
        }
    }
}


