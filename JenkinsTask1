pipeline {
    agent any

    stages {
        stage('build') {
            steps {
                bat 'Task.py ${env.JOB_NAME}'
            }
        }
        stage('test') {
            steps {
                echo 'Testing'
            }
        }
        stage('Moniter') {
            steps {
                echo 'monitering'
            }
        }
        stage('deploy') {
            steps {
                echo 'deploying'
            }
        }
    }
}
