    agent any

    stages {
        stage("Env Variables") {
            steps {
                bat "set"
            }
        }
        stage('build') {
            steps {
                echo 'building'
            }
        }
        stage('test') {
            steps {
                echo 'Testing'
            }
        }
        stage('Moniter') {
            steps {
                echo 'monitering'
            }
        }
        stage('deploy') {
            steps {
                echo 'deploying'
            }
        }
    }
}
