pipeline {
    agent any

    stages {
        stage('build') {
            steps {
                echo 'building'
            }
        }
        stage('test') {
            steps {
                echo 'Testing'
            }
        }
        stage('Moniter') {
            steps {
                echo 'monitering'
            }
        }
        stage('deploy') {
            steps {
                echo 'deploying'
            }
        }
        stage('python') {
            steps {
                bat 'lav.py'
            }
        }
        stage ('Build JobB'){
            steps{
                build job: 'Sample_Projects_pos', parameters: [string(name: "Environment", value: "production")]
            }
        }
    }
}
