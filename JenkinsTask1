pipeline {
    agent any

    stages {
        stage('build') {
            steps {
                echo 'hi'
            }
        }
        stage('test') {
            steps {
                echo 'Testing'
            }
        }
        stage('Moniter') {
            steps {
                echo 'monitering'
            }
        }
        stage('deploy') {
            steps {
                bat 'Task.py'
            }
        }
    }
}
