pipeline {
    environment {
        doError = '1'
    }
    agent any
        stages {
        stage('Bulding') {
            steps {
                bat 'shivam.py'
            }
        }
    }
    post {
        always {

                echo 'Status of build'
                emailext body: '', postsendScript: '''job name:
                Build number:''', subject: 'Job Name', to: 'dubeyshivam264@gmail.com'
        }
    }
}
